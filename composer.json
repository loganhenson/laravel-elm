{
  "name": "tightenco/laravel-elm",
  "description": "The Elm Laravel Platform",
  "keywords": [
    "laravel",
    "elm"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Logan Henson",
      "email": "logan@tighten.co"
    }
  ],
  "require": {
    "php": ">=7.4"
  },
  "require-dev": {
    "illuminate/support": "^8.0",
    "illuminate/console": "^8.0",
    "illuminate/http": "^8.0",
    "illuminate/routing": "^8.0",
    "illuminate/filesystem": "^8.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Tightenco\\Elm\\ElmServiceProvider"
      ],
      "aliases": {
        "Elm": "Tightenco\\Elm\\ElmFacade"
      }
    }
  },
  "autoload": {
    "psr-4": {
      "Tightenco\\Elm\\": "src/"
    }
  }
}
